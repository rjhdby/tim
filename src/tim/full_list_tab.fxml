<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<SplitPane dividerPositions="0.555" prefHeight="160.0" prefWidth="800" xmlns="http://javafx.com/javafx/8.0.112"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="tim.controller.FullListTabController">
    <items>
        <AnchorPane maxWidth="440.0" minHeight="0.0" minWidth="440.0" prefHeight="160.0"
                    prefWidth="440.0">
            <TableView fx:id="fullList" prefHeight="200.0" prefWidth="200.0"
                       AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                       AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <columns>
                    <TableColumn fx:id="flName" prefWidth="120.0" text="Наименование"/>
                    <TableColumn fx:id="flDate" prefWidth="200.0"
                                 text="Дата следующей проверки"/>
                    <TableColumn fx:id="flLoc" prefWidth="120.0" text="Расположение"/>
                </columns>
            </TableView>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
            <children>
                <GridPane hgap="10.0" vgap="10.0">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <TextField fx:id="searchName" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                    <TextField fx:id="searchLocation" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    <Label text="Наименование" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                    <Label text="Расположение" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                    <Button fx:id="delete" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="3"
                            mnemonicParsing="false" onAction="#delete" text="Удалить"/>
                    <Button fx:id="add" GridPane.columnIndex="0" GridPane.rowIndex="3" mnemonicParsing="false"
                            onAction="#add" text="Добавить"/>
                </GridPane>
            </children>
        </AnchorPane>
    </items>
</SplitPane>