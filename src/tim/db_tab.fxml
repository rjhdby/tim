<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<GridPane prefHeight="160.0" prefWidth="800" xmlns="http://javafx.com/javafx/8.0.112"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="tim.controller.DbTabController"
          hgap="10.0" vgap="10.0">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Button fx:id="unload"  GridPane.columnIndex="0" GridPane.rowIndex="0"
            mnemonicParsing="false" onAction="#unload" text="Выгрузить в файл"/>
    <Button fx:id="append"  GridPane.columnIndex="0" GridPane.rowIndex="1"
            mnemonicParsing="false" onAction="#append" text="Добавить из файла"/>
    <Button fx:id="reload" GridPane.columnIndex="0" GridPane.rowIndex="2"
            mnemonicParsing="false" onAction="#reload" text="Пересоздать базу из файла"/>
    <Button fx:id="clear" GridPane.columnIndex="0" GridPane.rowIndex="3"
            mnemonicParsing="false" onAction="#clear" text="Очистить базу"/>

</GridPane>
